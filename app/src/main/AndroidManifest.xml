<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartparkingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartParkingApp">
        <activity
            android:name=".SucesspayActivity"
            android:label="@string/title_activity_sucesspay"
            android:theme="@style/Theme.SmartParkingApp.NoActionBar"></activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".ReserFindActivity" />
        <activity android:name=".ParkInfoActivity" />
        <activity android:name=".ReserveActivity" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".FavActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".ConfParkActivity" />
        <activity android:name=".CheckpayActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>